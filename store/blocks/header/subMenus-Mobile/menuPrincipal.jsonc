{
    // Menu mobile
  "menu#drawer": {
    "children": [
      "flex-layout.row#menus-items",
      "flex-layout.row#postMenu"
    ],
    "props": {
      "blockClass":"menuM",
      "orientation": "vertical"
    }
  },
  // Menus items
  "flex-layout.row#menus-items": {
    "children": [
      "disclosure-layout#baterias-submenu-m-lvl2",
