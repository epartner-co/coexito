      "disclosure-layout#lubricantesM-submenu-m-lvl2",
      "disclosure-layout#autopartesM-submenu-m-lvl2",
      "disclosure-layout#motopartesM-submenu-m-lvl2"
    ],
    "props": {
      "blockClass": "subMneusM"
    }
  },
  // box final del menu
  "flex-layout.row#postMenu": {
    "children": [
      "flex-layout.col#imagenBanner",
      "flex-layout.col#menuItem1",
      "flex-layout.col#menuItem2",
      "flex-layout.col#menuItem3",
      "flex-layout.col#menuItem4"
    ],
    "props": {
      "blockClass": "post-menu"
    }
  },
  "flex-layout.col#imagenBanner": {
    "children": [
      "image#mobile-banner"
    ],
