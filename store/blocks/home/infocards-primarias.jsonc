{
"responsive-layout.desktop#info":{
  "children":[
    "flex-layout.row#contenedorinfo"
  ]
},
"responsive-layout.phone#info":{
  "children":[
    "flex-layout.row#contenedorinfoM"
  ]
},
"flex-layout.row#contenedorinfo": {
  "children": ["flex-layout.col#contenedorIN", "flex-layout.col#contenedorIN2", "flex-layout.col#contenedorIN3", "flex-layout.col#contenedorIN4"],
  "props":{
    "blockClass":"fichas"
  }
},
"flex-layout.row#contenedorinfoM": {
 // "children": ["flex-layout.col#contenedorINM"],
  "props":{
    "blockClass":"fichas"
  }
},
//Apertura contenedor de fichas
"flex-layout.col#contenedorIN":{
  "children":[
    "image#uno",
    "rich-text#uno"
  ],
  "props":{
    "blockClass":"fichas"
  }
},
"flex-layout.col#contenedorIN2":{
  "children":[
    "image#dos",
    "rich-text#dos"
  ],
  "props":{
    "blockClass":"fichas"
  }
},
"flex-layout.col#contenedorIN3":{
  "children":[
    "image#tres",
    "rich-text#tres"
  ],
  "props":{
    "blockClass":"fichas"
  }
},
"flex-layout.col#contenedorIN4":{
  "children":[
    "image#cuatro",
    "rich-text#cuatro"
  ],
  "props":{
    "blockClass":"fichas"
  }
},
//Imagenes de los botones
"image#uno": {
  "props": {
    "src": "https://coexitob2b.vteximg.com.br/arquivos/cotizar-desk.png",
    "maxHeight": 500
  }
},
"image#dos": {
  "props": {
    "src": "https://coexitob2b.vteximg.com.br/arquivos/promociones-desk.png",
    "maxHeight": 500
  }
},
"image#tres": {
  "props": {
    "src": "https://coexitob2b.vteximg.com.br/arquivos/estrategias-desk.png",
    "maxHeight": 500
  }
},
"image#cuatro": {
  "props": {
    "src": "https://coexitob2b.vteximg.com.br/arquivos/credito-desk.png",
    "maxHeight": 500
  }
},
