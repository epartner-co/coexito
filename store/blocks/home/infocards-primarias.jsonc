"rich-text#uno": {
  "props": {
    "textAlignment": "CENTER",
    "textPosition": "CENTER",
    "text": "\n**Cotizar**\nEmpieza tu orden ahora\n[Cotizar](/orderquote)",
    "textColor": "c-on-emphasis",
    "font": "t-heading-5",
    "blockClass": "textosC"
  }
},
"rich-text#dos": {
  "props": {
    "textAlignment": "CENTER",
    "textPosition": "CENTER",
    "text": "\n**Ver promociones**\nActualízate con nuestras ultimas campañas\n[Ver promociones](/140?map=productClusterIds&order=OrderByBestDiscountDESC)",
