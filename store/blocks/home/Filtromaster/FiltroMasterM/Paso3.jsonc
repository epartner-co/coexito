    "image#PesadoshoverMob": {
      "props": {
        "src": "https://coexitob2b.vteximg.com.br/arquivos/camion-hoverFiltro.png",
        "maxHeight": 240,
        "blockClass":["imagescompleH","llantasC"]
      }
    },
    "image#MotoshoverMob": {
      "props": {
        "src": "https://coexitob2b.vteximg.com.br/arquivos/motos-hoverFiltro.png",
        "maxHeight": 240,
        "blockClass":["imagescompleH","lubricantesC"]
      }
    },


  //Botones finales
  "flex-layout.col#botonesFprimerpaso2Mob":{
    "children":["flex-layout.col#botonprimerpaso3Mob","rich-text#botonPp33Mob", "rich-text#botonPp23Mob"],
    "props":{

      "blockClass":"bloqueprinci3",
      "width": "15%",
      "preserveLayoutOnMobile": true
