  },
  //Titulo paso 2
  "rich-text#titulopaso3Mob": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "**PASO 2:** ¿Para qué tipo de vehículo?",
      "blockClass": ["tituloP"]
    }
  },
  //Cuerpo de imagenes
  "flex-layout.row#imgprimerpaso3Mob":{
    "children": ["rich-text#titulopaso3Mob","slider-layout#filtroM"],
    "props":{
      "blockClass":"bloqueprinci2",
      "width": "70%"
    }
  },
  "slider-layout#filtroM": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 3,
        "phone": 3
      },
      "infinite": true,
      "showPaginationDots": "never",
      "showNavigationArrows": "always",
      "blockClass": "carouselCF"
    },
    "children": ["flex-layout.col#image5Mob" , "flex-layout.col#image6Mob", "flex-layout.col#image7Mob"]
  },


  "flex-layout.row#bloque3Mob":{
