        "blockClass":["image1", "imageb2"]
      }
    },
    "flex-layout.col#image6Mob":{
      "children":[
        "image#PesadosMob",
        "image#PesadoshoverMob",
        "rich-text#pesadosFMob"
      ],
      "props":{
        "blockClass":["image1", "imageb3"]
      }
    },
    "flex-layout.col#image7Mob":{
      "children":[
        "image#MotosMob",
        "image#MotoshoverMob",
        "rich-text#motosFMob"
      ],
      "props":{
        "blockClass":["image1", "imageb4"]
      }
    },

    //Texto en images
    "rich-text#livianosFMob": {
      "props": {
        "textAlignment": "CENTER",
        "textPosition": "CENTER",
        "text": "Livianos",
        "blockClass": ["botonbuscarFiltro","selectedD"]
      }
    },
    "rich-text#pesadosFMob": {
      "props": {
        "textAlignment": "CENTER",
        "textPosition": "CENTER",
        "text": "Pesados",
