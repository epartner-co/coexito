  "image#LivianosMob": {
    "props": {
      "src": "https://coexitob2b.vteximg.com.br/arquivos/carroFiltro.png",
      "maxHeight": 240,
      "blockClass":["imagescomple","bateriasC"]
    }
  },
  "image#PesadosMob": {
    "props": {
      "src": "https://coexitob2b.vteximg.com.br/arquivos/camionFiltro.png",
      "maxHeight": 240,
      "blockClass":["imagescomple","llantasC"]
    }
  },
  "image#MotosMob": {
    "props": {
      "src": "https://coexitob2b.vteximg.com.br/arquivos/motosFiltro.png",
